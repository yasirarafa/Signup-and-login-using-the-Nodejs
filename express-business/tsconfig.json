{
    "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib":["es6","dom","es2017"],
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "allowJs": true ,
    "checkJs": false ,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
    "": [
    "node_modules/",
    "src/types/"
    ]
    },
    },
    "exclude": ["dist"],
    "include": ["src","*.ts"],
    "types": ["src/types"]
    
}
